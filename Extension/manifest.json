{
  "manifest_version": 2,

  "name": "WeGotU",
  "description": "This extension tracks your internet content emotion",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
  ],
  "background": {
    "scripts": ["pageParser.js"]
  }
}